	<bean id="triangle" class="okasha.Triangle" name="triangle-name">
		<property name="pointA">
			<idref = zeroPoint"/>
		</property>
		<property name="pointB">
			<bean id="point2" class="okasha.Point">

				<property name="x" value="-20" />
				<property name="y" value="0" />
			</bean>
		</property>
		<property name="pointC">

			<bean id="point3" class="okasha.Point">

				<property name="x" value="20" />
				<property name="y" value="0" />
			</bean>
		</property>
	</bean>
	<bean id="zeroPoint" class="okasha.Point">

		<property name="x" value="0" />
		<property name="y" value="0" />
	</bean>
	<alias name="triangle" alias="triangle-alias" />
</beans>